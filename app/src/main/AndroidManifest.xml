<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.migration">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Register.ForgotPass"></activity>
        <activity android:name=".Launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@drawable/button" />

        <activity
            android:name=".Register.RegisterUser"
            android:parentActivityName=".Register.LoginOrRegister" />
        <activity
            android:name=".Register.LoginOrRegister"
            android:parentActivityName=".Register.select_login_type" />
        <activity
            android:name=".Questions.questions"
            android:label="@string/title_activity_questions"
            android:parentActivityName=".Register.select_login_type"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".data.data_main" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".Register.select_login_type" />
        <activity android:name=".Stats.stats_main" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>